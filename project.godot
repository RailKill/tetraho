[WAT]

Test_Directory="res://tests"
Results_Directory="res://tests/results/WAT"
Minimize_Window_When_Running_Tests=false
TestStrategy={
"repeat": 1,
"strategy": "RunAll"
}
Tags=PoolStringArray(  )
Display=8

[application]

config/name="tetraho"
config/description="A game of block wizardry."
run/main_scene="res://assets/scenes/ui/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

Configuration="*res://assets/scripts/configuration.gd"
GameWorld="*res://assets/scenes/world.tscn"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=1280
window/size/height=720
mouse_cursor/custom_image="res://assets/sprites/ui/menu/cursor.png"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "WAT" )

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

